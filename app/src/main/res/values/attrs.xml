<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>

    <declare-styleable name="FabWithTitleView">
        <attr name="fab_title" format="string" />
        <attr name="fab_color" format="color" />
        <attr name="fab_drawable" format="reference" />
    </declare-styleable>

    <declare-styleable name="OptionsFabLayout">
        <attr name="options_menu" format="reference" />
        <attr name="options_color" format="color" />
        <attr name="src" format="reference" />
        <attr name="color" format="color" />
        <attr name="background_color" format="color" />
    </declare-styleable>
</resources>